# Ignore temporary files from editors and OS
.DS_Store
Thumbs.db

# Ignore local environment files
.env
.env.local
.env.*.local

# Ignore build output
/dist
/build
/out
/.next

# Ignore Yarn's default install directories
.pnp.*
.yarn/*
.yarn/unplugged
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/sdks
!.yarn/versions
!.yarn/cache


# Ignore logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore IDE-specific settings
.idea/
.vscode/
*.iml
node_modules
